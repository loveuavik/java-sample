pipeline {
    agent any
	parameters {
        string(defaultValue: "", description: '', name: 'ARTIFACTID')
        string(defaultValue: "", description: '', name: 'VERSION')
        string(defaultValue: "", description: '', name: 'GROUPID')
    }
	stages {
		stage('Deploy to Server') {
			steps {
					sh '''
					echo "${GROUPID}"
					echo "${ARTIFACTID}"
					echo "${VERSION}" 
					'''
			}
		}
	}
}
